{
  "name": "i15AI",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "jest",

    "api:dev": "uvicorn backend.server:app --host 0.0.0.0 --port 8001 --reload",
    "api:start": "uvicorn backend.server:app --host 0.0.0.0 --port 8001",

    "web:build": "npm --prefix web-app ci && npm --prefix web-app run build",
    "web:start:prod": "npx --yes serve -s web-app/build -l tcp://0.0.0.0:3000",

    "pwa:build": "npm run web:build",
    "pwa:serve": "npm run web:start:prod"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.72.4",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/stack": "^6.3.16",
    "axios": "^1.6.7",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/runtime": "^7.23.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "babel-jest": "^29.5.0",
    "eslint": "^8.50.0",
    "jest": "^29.5.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "react-test-renderer": "18.2.0"
  },
  "jest": {
    "preset": "react-native"
  }
}